<Window x:Class="SortedListApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SortedListApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="160" Width="400">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="41" Margin="10,10,0,0" TextWrapping="Wrap" Text="key" VerticalAlignment="Top" Width="214" FontSize="24"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="41" Margin="10,70,0,0" TextWrapping="Wrap" Text="value" VerticalAlignment="Top" Width="214" FontSize="24"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="254,38,0,0" VerticalAlignment="Top" Width="120" Height="53" Click="Add_Button_Click"/>

    </Grid>
</Window>
